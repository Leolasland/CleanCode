for (int i = 0; i < (N - 1); i++) {
  if (startImpulse[i] > startImpulse[i + 1]) {
    x = startImpulse[i];
    startImpulse[i] = startImpulse[i + 1];
    startImpulse[i + 1] = x;
    change = true;
  }
}
